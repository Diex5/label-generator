{
  "name": "pb-nuxt",
  "version": "1.1.0",
  "private": true,
  "packageManager": "pnpm@9.12.3",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && pnpm simple-git-hooks install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "nuxt typecheck --no-emit",
    "taze:up": "taze major -I",
    "test:unit": "vitest",
    "cleanup": "nuxt cleanup && pnpm dedupe",
    "release:bump": "changelogen --bump",
    "release": "npx changelogen --release --push",
    "prisma-seed": "tsx prisma/seed.ts"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@iconify-json/heroicons": "^1.1.24",
    "@iconify-json/svg-spinners": "^1.1.3",
    "@iconify-json/token": "1.2.4",
    "@nuxt/devtools": "^1.4.1",
    "@nuxt/eslint": "0.5.3",
    "@nuxt/image": "^1.7.0",
    "@nuxt/test-utils": "^3.14.1",
    "@nuxtjs/color-mode": "^3.4.4",
    "@nuxtjs/supabase": "^1.4.1",
    "@pinia/nuxt": "^0.5.4",
    "@primevue/nuxt-module": "^4.2.1",
    "@prisma/client": "5.22.0",
    "@unocss/nuxt": "^0.62.4",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/nuxt": "^11.2.0",
    "eslint": "^9.9.1",
    "happy-dom": "^15.0.0",
    "lint-staged": "^15.2.9",
    "magic-regexp": "^0.8.0",
    "nuxt": "^3.13.0",
    "nuxt-headlessui": "^1.2.0",
    "nuxt-i18n-micro": "^1.31.2",
    "nuxt-radash": "^1.0.0",
    "pinia": "^2.2.2",
    "primeicons": "^7.0.0",
    "prisma": "5.22.0",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.16.7",
    "tsx": "^4.19.2",
    "typescript": "^5.5.4",
    "unocss": "^0.64.0",
    "viem": "^2.21.39",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.0.29"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "simple-git-hooks": {
    "commit-msg": "npx commitlint --edit $1",
    "pre-commit": "npx lint-staged"
  },
  "overrides": {
    "vue": "latest"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.8.2",
    "@primevue/themes": "^4.2.1",
    "@prisma/nuxt": "^0.0.35",
    "@vueuse/core": "^11.2.0",
    "konva": "^9.3.16",
    "primevue": "^4.2.1",
    "superjson": "^2.2.1",
    "vue-konva": "^3.1.2"
  }
}